{
  "name": "design-systems-directory",
  "description": "All the design system articles, videos, events, and communities in one place",
  "version": "1.0.0",
  "author": "Ryosuke <ryosuke.san.hana@gmail.com>",
  "keywords": [
    "design-systems",
    "component-library",
    "react",
    "mdx",
    "storybook",
    "styled-components",
    "gatsby",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache && npm run develop",
    "develop": "nodemon --exec \"gatsby develop\"",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint . --ext ts --ext tsx",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*",
      "content/*",
      "static/*"
    ],
    "delay": "1500"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.0",
    "@mdx-js/react": "^1.5.0",
    "@mdx-js/tag": "^0.20.3",
    "@rebass/forms": "^4.0.6",
    "array-flat-polyfill": "^1.0.1",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.15.28",
    "gatsby-image": "^2.2.23",
    "gatsby-link": "^2.2.17",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-feed": "^2.3.15",
    "gatsby-plugin-google-analytics": "^2.1.19",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-mdx": "^1.0.46",
    "gatsby-plugin-offline": "^3.0.11",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-sharp": "^2.2.27",
    "gatsby-plugin-sitemap": "^2.2.16",
    "gatsby-plugin-styled-components": "^3.1.8",
    "gatsby-plugin-twitter": "^2.1.9",
    "gatsby-remark-copy-linked-files": "^2.1.23",
    "gatsby-remark-images": "^3.1.25",
    "gatsby-remark-prismjs": "^3.3.16",
    "gatsby-remark-smartypants": "^2.1.11",
    "gatsby-source-filesystem": "^2.1.28",
    "gatsby-transformer-json": "^2.2.13",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.2.19",
    "prism-react-renderer": "^1.0.1",
    "react": "^16.10.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-data-table-component": "^3.5.1",
    "react-dom": "^16.10.1",
    "react-feather": "^2.0.8",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.1",
    "react-mailchimp-subscribe": "^2.1.0",
    "rebass": "^4.0.6",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-backgrounds": "^5.2.1",
    "@storybook/addon-events": "^5.2.1",
    "@storybook/addon-info": "^5.2.1",
    "@storybook/addon-jest": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-notes": "^5.2.1",
    "@storybook/addon-options": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.1",
    "@storybook/addon-viewport": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "@types/react": "^16.9.4",
    "@types/react-dom": "^16.9.1",
    "@types/rebass": "^4.0.1",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.19",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-graphql": "^3.1.0",
    "eslint-plugin-react": "^7.15.0",
    "gatsby-plugin-typescript": "^2.1.11",
    "gh-pages": "^2.1.1",
    "graphql-typescript-definitions": "^0.17.3",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3",
    "typescript-styled-plugin": "^0.14.0"
  }
}
